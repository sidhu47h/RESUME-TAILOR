{
    "version": 2,
    "functions": {
      "resume_tailor.py": {
        "maxDuration": 30
      }
    },
    "builds": [
      {
        "src": "resume_tailor.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "resume_tailor.py"
      }
    ]
  }
  